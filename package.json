{
  "name": "oracledb",
  "version": "1.7.1",
  "description": "Oracle Database driver by Oracle Corp.",
  "license": "Apache-2.0",
  "homepage": "http://www.oracle.com/technetwork/database/database-technologies/scripting-languages/node_js/",
  "keywords": [
    "oracledb",
    "dboracle",
    "Node.js",
    "SQL",
    "PL/SQL",
    "connection",
    "connectivity",
    "OCI",
    "client",
    "plugin",
    "library",
    "driver",
    "add-on",
    "extension",
    "binding",
    "interface",
    "adapter",
    "module",
    "DB",
    "official",
    "Database",
    "Oracle"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/oracle/node-oracledb.git"
  },
  "dependencies": {
    "nan": "~2.1.0",
    "node-pre-gyp": "0.6.x"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "devDependencies": {
      "mocha": "~2.2.5",
      "should": "~6.0.1",
      "async": "~0.9.0"
  },
  "scripts": {
      "preinstall": "npm install node-pre-gyp",
      "install": "node-pre-gyp install --fallback-to-build",
      "test": "mocha -R spec --timeout 10000 test/*.js",
      "testWindows": "mocha -R spec --timeout 0 test\\*.js"
  },
  "binary": {
    "module_name": "oracledb",
    "module_path": "./build/Release",
    "host": "https://raw.githubusercontent.com/bchr02/node-oracledb/master/bin",
    "remote_path": "",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz"
  },
  "engines": {
    "node": ">=0.10.28 <6"
  },
  "maintainers": [
    {
      "name": "Oracle Corp."
    }
  ],
  "bugs": {
    "url": "https://github.com/oracle/node-oracledb/issues"
  },
  "main": "./index.js"
}
